{
  "annotations": {"list": []},
  "panels": [
    {
      "type": "stat",
      "title": "Executor: Tasks Executed",
      "gridPos": {"h": 6, "w": 6, "x": 0, "y": 0},
      "targets": [
        {"expr": "syndicate_executor_tasks_total"}
      ]
    },
    {
      "type": "stat",
      "title": "Executor: Success Rate",
      "gridPos": {"h": 6, "w": 6, "x": 6, "y": 0},
      "targets": [
        {"expr": "(syndicate_executor_tasks_succeeded / syndicate_executor_tasks_total) * 100"}
      ]
    },
    {
      "type": "graph",
      "title": "Executor: Task Latency (ms)",
      "gridPos": {"h": 8, "w": 12, "x": 0, "y": 6},
      "targets": [
        {"expr": "histogram_quantile(0.95, sum(rate(syndicate_executor_task_duration_seconds_bucket[5m])) by (le))"}
      ]
    },
    {
      "type": "graph",
      "title": "LLM: Token Usage (per provider)",
      "gridPos": {"h": 8, "w": 12, "x": 12, "y": 6},
      "targets": [
        {"expr": "sum(rate(syndicate_llm_tokens_total[5m])) by (provider)"}
      ]
    },
    {
      "type": "graph",
      "title": "System: DB Connections / Busy",
      "gridPos": {"h": 6, "w": 12, "x": 0, "y": 14},
      "targets": [
        {"expr": "syndicate_db_busy_total"}
      ]
    }
  ],
  "schemaVersion": 30,
  "title": "Syndicate - Executor Overview",
  "version": 1
}
